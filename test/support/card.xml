<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:cert="http://www.w3.org/ns/auth/cert#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <foaf:Person rdf:about="#me">
        <cert:key>
            <cert:RSAPublicKey>
                <cert:exponent rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">65537</cert:exponent>
                <cert:modulus rdf:datatype="http://www.w3.org/2001/XMLSchema#hexBinary">abcdef</cert:modulus>
            </cert:RSAPublicKey>
        </cert:key>
        <foaf:name xml:lang="en">John Smith</foaf:name>
    </foaf:Person>
</rdf:RDF>